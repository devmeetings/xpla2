{
  "name": "xplatform",
  "version": "3.0.0",
  "description": "XPlatform light",
  "author": "Tomusdrw <tomusdrw@gmail.com>",
  "private": true,
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "localhost",
    "devPort": 3010
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-ci": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-ci": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "brace": "0.9.1",
    "consolidate": "0.14.5",
    "event-emitter": "0.3.4",
    "immutable": "3.8.1",
    "isomorphic-fetch": "2.2.1",
    "jszip": "3.1.3",
    "lodash": "4.17.4",
    "normalize.css": "5.0.0",
    "rc-tooltip": "3.4.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-immutable-proptypes": "2.1.0",
    "react-modal": "1.6.5",
    "react-redux": "5.0.2",
    "react-treeview": "0.4.6",
    "redux": "3.6.0",
    "redux-actions": "1.2.0",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "2.2.0",
    "webcomponents.js": "0.7.23"
  },
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-rewire": "1.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-runtime": "6.22.0",
    "babel-template": "6.22.0",
    "babel-types": "6.22.0",
    "chai": "3.5.0",
    "css-loader": "0.26.1",
    "eslint": "3.14.0",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "glob": "7.1.1",
    "html-loader": "0.4.4",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sinon": "1.0.5",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.1",
    "mocha": "3.2.0",
    "mocha-loader": "1.1.0",
    "node-libs-browser": "2.0.0",
    "node-sass": "4.3.0",
    "opn": "4.0.2",
    "phantomjs-prebuilt": "2.1.14",
    "postcss-loader": "1.2.2",
    "raw-loader": "0.5.1",
    "react-hot-loader": "1.3.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "sinon": "1.17.7",
    "source-map-support": "0.4.10",
    "style-loader": "0.13.1",
    "template-html-loader": "1.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "globals": {
      "describe": true,
      "it": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": [
        2,
        "global"
      ],
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-alert": 0,
      "no-underscore-dangle": 0,
      "react/display-name": 0,
      "jsx-quotes": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-sort-props": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/jsx-wrap-multilines": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1
    }
  }
}
